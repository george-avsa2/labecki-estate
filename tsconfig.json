{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "Assets/*": ["src/assets/*"],
      "App/*": ["src/app/*"],
      "Components/*": ["src/components/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["./dist"]
}
